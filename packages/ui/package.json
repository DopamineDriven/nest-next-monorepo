{
  "name": "ui",
  "version": "1.0.0",
  "main": "index.tsx",
  "types": "**/*.{d.ts|ts|tsx}",
  "author": "Andrew Ross",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@headlessui/react": "^1.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/tailwindcss": "^3.0.10",
    "autoprefixer": "^10.4.4",
    "classnames": "^2.3.1",
    "config": "*",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.0.23",
    "tsconfig": "*",
    "typescript": "^4.6.3",
    "unwrap-react": "^0.4.0"
  },
  "peerDependencies": {
    "next": "^12.1.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "useTabs": false,
    "tabWidth": 2,
    "bracketSameLine": true,
    "jsxSingleQuote": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "bracketSpacing": true,
    "quoteProps": "as-needed",
    "printWidth": 80,
    "overrides": [
      {
        "files": [
          "**/*.{yaml|yml}"
        ],
        "options": {
          "singleQuote": false
        }
      },
      {
        "files": [
          "**/*.{module.css|css}"
        ],
        "options": {
          "printWidth": 60
        }
      }
    ]
  }
}
